/* pages/list/index.wxss */
.list {
  width: 100%;
}
.list .listFilter {
  width: 100%;
  height: 90rpx;
  border:1px solid red;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: rgba(51,51,51,1);
}
.list .listFilter .listFilter-item {
  width: 187rpx;
  text-align: center;
  border:1px solid red;
}
.list .listFilter image {
  width: 12rpx;
  height: 7.5rpx;
}